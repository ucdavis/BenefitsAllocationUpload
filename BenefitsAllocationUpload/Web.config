    <roleManager enabled="true" defaultProvider="CatbertRoleProvider" cacheRolesInCookie="false">
      <providers>
        <add name="CatbertRoleProvider" type="UCDArch.Web.Providers.CatbertRoleProvider" applicationName="BenefitsAllocationUpload" description="BenefiteAllocationUpload" connectionString="Catbert"/>
      </providers>
